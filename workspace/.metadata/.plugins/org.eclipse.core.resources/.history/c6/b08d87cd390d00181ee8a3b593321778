package planta;

import archivos.Matriz;
import monitor.GestorMonitor;

public class Linea2 implements Runnable {
	
	private GestorMonitor gm;
	private Matriz secuencia;

	
	public Linea2(GestorMonitor gm, Matriz s) {
		this.gm = gm;
		this.secuencia = s;
	}
	
	@Override
	public void run() {
		for(;;){//int j=0;j<500;j++) {
			int i = 0;
			while (i < secuencia.getColumnas()) {
				//gm.dispararTransicion(secuencia[i]);
				i++;
			}
		}
	}

}